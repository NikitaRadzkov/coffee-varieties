{
	"name": "coffee-varieties",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "yarn check:env && vite dev",
		"build": "yarn check:env && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:env": "tsx src/lib/config/config.ts",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-vercel": "^5.7.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/node": "^22.14.0",
		"autoprefixer": "^10.4.21",
		"dotenv": "^16.4.7",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"picomatch": "^4.0.0",
		"postcss": "^8.5.3",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.3",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.5",
		"vitest": "^3.0.0",
		"yaml": "^2.4.2",
		"zod": "^3.24.2"
	},
	"dependencies": {
		"lru-cache": "^11.1.0",
		"node-cache": "^5.1.2",
		"pino": "^9.6.0",
		"pino-pretty": "^13.0.0"
	}
}
